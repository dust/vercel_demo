{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@ardnt/vercel-python-wsgi",
          "config": {
              "runtime": "python3.9"
          }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.py"
      }
    ]
  }